	[('a', 'b'), ('a', 'c'), ('a', 'd'), ('b', 'c'), ('b', 'd'), ('c', 'd')] 
	res = [''.join(c) for c in combs]
	['ab', 'ac', 'ad', 'bc', 'bd', 'cd']

